{
    "name": "lindesbs/memberdisplay",
    "type": "contao-bundle",
    "description": "MemberDisplay",
    "homepage": "https://ktrion.de.de",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Stefan Lindecke",
            "homepage": "https://lindesbs.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.8"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "extra": {
        "contao-manager-plugin": "lindesbs\\MemberDisplay\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "lindesbs\\MemberDisplay\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/assets",
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "lindesbs\\MemberDisplay\\Tests\\": "tests/"
        }
    }
}
